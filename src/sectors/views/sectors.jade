.row
  .col.s12(ng-repeat='sector in sectors.sectors track by sector.id')
    .card.blue-grey.darken-1
      .card-content.white-text
        span.card-title Sector: {{sector.name}}
      .card-action
        a(href='#') Medido: {{sector.completed}}%
        a(ui-sref='medicion.plots({idsector: sector.id})') Ingresar